{
  "compilerOptions": {
    "paths": {},
    "strict": true,
    "strictNullChecks": true,
    "jsx": "preserve",
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "module": "ES6",
    "moduleResolution": "Node",
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "composite": true,
    "target": "ES6",
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
  },
